{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ozon-tracker",
  "type": "module",
  "version": "0.1.1",
  "private": true,
  "packageManager": "yarn@4.9.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.cjs",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "release": "yarn publish",
    "release:changelog": "changelogen --release --output CHANGELOG.md",
    "release:patch": "changelogen --release --bump patch",
    "release:minor": "changelogen --release --bump minor",
    "release:major": "changelogen --release --bump major",
    "changelog": "changelogen --output CHANGELOG.md",
    "lint": "eslint --fix --ext=ts,js,vue .",
    "clean": "yarn exec rm -rf dist"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "dotenv": "^16.5.0",
    "level": "^8.0.1",
    "playwright": "^1.52.0"
  },
  "devDependencies": {
    "@types/levelup": "^5.1.5",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@viapip/eslint-config": "^0.1.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@yarnpkg/doctor": "^4.0.3",
    "changelogen": "^0.6.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.26.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.5.2",
    "pkgroll": "^2.12.2",
    "puppeteer": "^22.15.0",
    "simple-git-hooks": "^2.13.0",
    "telegraf": "^4.16.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --fix"
  },
  "dependenciesMeta": {
    "@rollup/plugin-typescript@11.1.1": {
      "unplugged": true
    },
    "rollup-plugin-esbuild@5.0.0": {
      "unplugged": true
    }
  }
}
